            function renderBackgroundParticles() {
                particles.forEach(particle => {
                    particle.x += particle.speedX;
                    particle.y += particle.speedY;
                    particle.wave += 0.02;
                    
                    particle.x += Math.sin(particle.wave) * 0.3;
                    particle.y += Math.cos(particle.wave * 0.7) * 0.3;
                    
                    if (particle.x < -20) particle.x = canvas.width + 20;
                    if (particle.x > canvas.width + 20) particle.x = -20;
                    if (particle.y < -20) particle.y = canvas.height + 20;
                    if (particle.y > canvas.height + 20) particle.y = -20;
                    
                    ctx.beginPath();
                    ctx.arc(particle.x, particle.y, particle.size, 0, Math.PI * 2);
                    ctx.fillStyle = particle.color;
                    ctx.fill();
                });
            }
            
            // Ø±Ù†Ø¯Ø± Ø¢ØªØ´â€ŒØ¨Ø§Ø²ÛŒ
            function renderFireworks() {
                fireworks = fireworks.filter(fw => fw.life > 0);
                
                fireworks.forEach(fw => {
                    fw.x += fw.speedX;
                    fw.y += fw.speedY;
                    fw.speedY += fw.gravity;
                    fw.life--;
                    
                    fw.trail.push({x: fw.x, y: fw.y});
                    if (fw.trail.length > 6) fw.trail.shift();
                    
                    // Ø±Ø³Ù… Ø¯Ù†Ø¨Ø§Ù„Ù‡
                    if (fw.trail.length > 1) {
                        ctx.strokeStyle = fw.color.replace(')', ', 0.3)').replace('rgb', 'rgba');
                        ctx.lineWidth = fw.size / 2;
                        ctx.lineCap = 'round';
                        
                        ctx.beginPath();
                        ctx.moveTo(fw.trail[0].x, fw.trail[0].y);
                        for (let i = 1; i < fw.trail.length; i++) {
                            ctx.lineTo(fw.trail[i].x, fw.trail[i].y);
                        }
                        ctx.stroke();
                    }
                    
                    // Ø±Ø³Ù… Ø°Ø±Ù‡
                    const alpha = fw.life / fw.maxLife;
                    const gradient = ctx.createRadialGradient(
                        fw.x, fw.y, 0,
                        fw.x, fw.y, fw.size * 2
                    );
                    
                    gradient.addColorStop(0, fw.color.replace(')', ', 0.9)').replace('rgb', 'rgba'));
                    gradient.addColorStop(0.7, fw.color.replace(')', ', 0.4)').replace('rgb', 'rgba'));
                    gradient.addColorStop(1, fw.color.replace(')', ', 0)').replace('rgb', 'rgba'));
                    
                    ctx.fillStyle = gradient;
                    ctx.beginPath();
                    ctx.arc(fw.x, fw.y, fw.size * 2, 0, Math.PI * 2);
                    ctx.fill();
                    
                    // Ù‡Ø³ØªÙ‡ Ù…Ø±Ú©Ø²ÛŒ
                    ctx.fillStyle = 'rgba(255, 255, 255, 0.7)';
                    ctx.beginPath();
                    ctx.arc(fw.x, fw.y, fw.size / 1.8, 0, Math.PI * 2);
                    ctx.fill();
                });
            }
            
            // Ø±Ù†Ø¯Ø± Ù‚Ù„Ø¨â€ŒÙ‡Ø§
            function renderHearts() {
                hearts = hearts.filter(heart => heart.life > 0);
                
                hearts.forEach(heart => {
                    heart.x += heart.speedX;
                    heart.y += heart.speedY;
                    heart.speedY += heart.gravity;
                    heart.rotation += heart.rotationSpeed;
                    heart.life--;
                    
                    const alpha = heart.life / heart.maxLife;
                    
                    ctx.save();
                    ctx.translate(heart.x, heart.y);
                    ctx.rotate(heart.rotation);
                    ctx.scale(1, 1);
                    
                    ctx.fillStyle = heart.color.replace(')', `, ${alpha})`).replace('rgb', 'rgba');
                    ctx.strokeStyle = 'rgba(255, 255, 255, 0.5)';
                    ctx.lineWidth = 1.2;
                    
                    // Ø±Ø³Ù… Ù‚Ù„Ø¨
                    ctx.beginPath();
                    ctx.moveTo(0, heart.size / 4);
                    
                    // Ø³Ù…Øª Ú†Ù¾ Ù‚Ù„Ø¨
                    ctx.bezierCurveTo(
                        -heart.size / 2, -heart.size / 2,
                        -heart.size, heart.size / 3,
                        0, heart.size
                    );
                    
