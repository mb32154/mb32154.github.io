                    ctx.bezierCurveTo(
                        heart.size, heart.size / 3,
                        heart.size / 2, -heart.size / 2,
                        0, heart.size / 4
                    );
                    
                    ctx.closePath();
                    ctx.fill();
                    ctx.stroke();
                    
                    ctx.restore();
                });
            }
            
            // Ø­Ù„Ù‚Ù‡ Ø§Ù†ÛŒÙ…ÛŒØ´Ù†
            function animate() {
                ctx.fillStyle = 'rgba(26, 26, 46, 0.1)';
                ctx.fillRect(0, 0, canvas.width, canvas.height);
                
                renderBackgroundParticles();
                renderFireworks();
                renderHearts();
                
                frameCount++;
                
                // Ø§ÛŒØ¬Ø§Ø¯ Ø§ÙÚ©Øª Ø®ÙˆØ¯Ú©Ø§Ø±
                if (frameCount % 180 === 0 && isLoaded) {
                    const x = Math.random() * canvas.width;
                    const y = Math.random() * canvas.height * 0.4 + canvas.height * 0.2;
                    createFirework(x, y);
                }
                
                animationId = requestAnimationFrame(animate);
            }
            
            // Ù¾Ø®Ø´ ØµØ¯Ø§
            function playSound(soundType) {
                try {
                    if (soundType === 'firework') {
                        const sound = document.getElementById('fireworkSound');
                        sound.currentTime = 0;
                        sound.volume = 0.3;
                        sound.play().catch(() => {});
                    } else if (soundType === 'heart') {
                        const sound = document.getElementById('heartSound');
                        sound.currentTime = 0;
                        sound.volume = 0.2;
                        sound.play().catch(() => {});
                    }
                } catch (error) {
                    console.log('Ø®Ø·Ø§ÛŒ ØµØ¯Ø§:', error);
                }
            }
            
            // Ú©Ù†ØªØ±Ù„ ÙˆÛŒØ¯ÛŒÙˆ
            function setupVideoControls() {
                const videoModal = document.getElementById('videoModal');
                const playVideoBtn = document.getElementById('playVideoBtn');
                const closeVideoBtn = document.getElementById('closeVideo');
                const modalVideo = document.getElementById('modalVideo');
                
                playVideoBtn.addEventListener('click', () => {
                    videoModal.classList.add('active');
                    // Ù¾Ø®Ø´ Ø®ÙˆØ¯Ú©Ø§Ø± ÙˆÛŒØ¯ÛŒÙˆ
                    modalVideo.src = "https://drive.google.com/file/d/1h4TRB2hVl6tKfcmBU6jyNHzNVJs0T_inwEOj1dQoU3s/preview?autoplay=1";
                });
                
                closeVideoBtn.addEventListener('click', () => {
                    videoModal.classList.remove('active');
                    // Ù…ØªÙˆÙ‚Ù Ú©Ø±Ø¯Ù† ÙˆÛŒØ¯ÛŒÙˆ
                    modalVideo.src = "https://drive.google.com/file/d/1h4TRB2hVl6tKfcmBU6jyNHzNVJs0T_inwEOj1dQoU3s/preview";
                });
                
                videoModal.addEventListener('click', (e) => {
                    if (e.target === videoModal) {
                        videoModal.classList.remove('active');
                        modalVideo.src = "https://drive.google.com/file/d/1h4TRB2hVl6tKfcmBU6jyNHzNVJs0T_inwEOj1dQoU3s/preview";
                    }
                });
            }
            
            // Ø±ÙˆÛŒØ¯Ø§Ø¯ Ú©Ù„ÛŒÚ© Ø¨Ø±Ø§ÛŒ Ø§ÛŒØ¬Ø§Ø¯ Ø¢ØªØ´â€ŒØ¨Ø§Ø²ÛŒ
            function setupClickEvents() {
                document.addEventListener('click', (e) => {
                    if (e.target.tagName !== 'BUTTON' && isLoaded) {
                        createFirework(e.clientX, e.clientY);
                    }
                });
                
                // Ø¯Ùˆ Ø¨Ø§Ø± Ú©Ù„ÛŒÚ© Ø¨Ø±Ø§ÛŒ Ù‚Ù„Ø¨
                let lastClick = 0;
                document.addEventListener('click', (e) => {
                    const now = Date.now();
                    if (now - lastClick < 300 && e.target.tagName !== 'BUTTON' && isLoaded) {
                        createHeartExplosion(e.clientX, e.clientY);
                    }
                    lastClick = now;
                });
            }
            
            // Ø±Ø§Ù‡â€ŒØ§Ù†Ø¯Ø§Ø²ÛŒ
            function init() {
                resizeCanvas();
                window.addEventListener('resize', resizeCanvas);
                
                simulateLoading();
                createBackgroundParticles();
                setupVideoControls();
                setupClickEvents();
                
                animate();
                
                console.log('âœ… Ø§ÙÚ©Øªâ€ŒÙ‡Ø§ÛŒ Ø±ÙˆØ² Ù¾Ø¯Ø± Ø¢Ù…Ø§Ø¯Ù‡ Ø´Ø¯!');
