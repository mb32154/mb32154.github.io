                setTimeout(() => createFirework(canvas.width / 4, canvas.height / 2), 400);
                setTimeout(() => createFirework(canvas.width * 3/4, canvas.height / 2), 800);
                
                // Ø§ÛŒØ¬Ø§Ø¯ Ù‚Ù„Ø¨â€ŒÙ‡Ø§
                setTimeout(() => createHeartExplosion(canvas.width / 2, canvas.height / 2), 1200);
                
                // Ø§Ø¯Ø§Ù…Ù‡ Ø§ÙÚ©Øªâ€ŒÙ‡Ø§
                setInterval(() => {
                    if (fireworks.length < 5) {
                        const x = Math.random() * canvas.width;
                        const y = Math.random() * canvas.height * 0.4 + canvas.height * 0.2;
                        createFirework(x, y);
                    }
                }, 3000);
                
                setInterval(() => {
                    if (hearts.length < 3) {
                        const x = Math.random() * canvas.width;
                        const y = canvas.height;
                        createFloatingHeart(x, y);
                    }
                }, 2000);
            }
            
            // Ø§ÛŒØ¬Ø§Ø¯ Ø¢ØªØ´â€ŒØ¨Ø§Ø²ÛŒ
            function createFirework(x, y) {
                const colors = ['#ff3366', '#33cc99', '#3366ff', '#ffcc00', '#ff33cc', '#33ffcc'];
                const particleCount = 80 + Math.random() * 40;
                
                for (let i = 0; i < particleCount; i++) {
                    const angle = Math.random() * Math.PI * 2;
                    const speed = 1 + Math.random() * 4;
                    const color = colors[Math.floor(Math.random() * colors.length)];
                    
                    fireworks.push({
                        x: x,
                        y: y,
                        size: Math.random() * 2.5 + 1,
                        speedX: Math.cos(angle) * speed,
                        speedY: Math.sin(angle) * speed,
                        color: color,
                        life: 50 + Math.random() * 30,
                        maxLife: 50 + Math.random() * 30,
                        gravity: 0.05,
                        trail: []
                    });
                }
                
                // ØµØ¯Ø§
                playSound('firework');
            }
            
            // Ø§ÛŒØ¬Ø§Ø¯ Ø§Ù†ÙØ¬Ø§Ø± Ù‚Ù„Ø¨
            function createHeartExplosion(x, y) {
                for (let i = 0; i < 30; i++) {
                    hearts.push({
                        type: 'heart',
                        x: x,
                        y: y,
                        size: Math.random() * 12 + 8,
                        speedX: (Math.random() - 0.5) * 6,
                        speedY: (Math.random() - 0.5) * 6 - 3,
                        color: '#ff3366',
                        rotation: Math.random() * Math.PI * 2,
                        rotationSpeed: (Math.random() - 0.5) * 0.08,
                        life: 80,
                        maxLife: 80,
                        gravity: 0.03
                    });
                }
                playSound('heart');
            }
            
            // Ø§ÛŒØ¬Ø§Ø¯ Ù‚Ù„Ø¨ Ø´Ù†Ø§ÙˆØ±
            function createFloatingHeart(x, y) {
                hearts.push({
                    type: 'floating',
                    x: x,
                    y: y,
                    size: Math.random() * 15 + 10,
                    speedX: (Math.random() - 0.5) * 2,
                    speedY: -2 - Math.random() * 2,
                    color: '#ff3366',
                    rotation: Math.random() * Math.PI * 2,
                    rotationSpeed: (Math.random() - 0.5) * 0.05,
                    life: 150,
                    maxLife: 150,
                    gravity: 0.01
                });
            }
            
